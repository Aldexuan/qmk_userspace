// clang-format on
//用来检测自动切换鼠标层的地方，如果鼠标的水平位移x或垂直位移y的绝对值大于设定的阈值
//自动切换到LAYER_POINTER鼠标层，并将rgb改为绿色
//如果没有开启这个不起作用
#ifdef POINTING_DEVICE_ENABLE
#    ifdef CHARYBDIS_AUTO_POINTER_LAYER_TRIGGER_ENABLE
report_mouse_t pointing_device_task_user(report_mouse_t mouse_report) {
    if (abs(mouse_report.x) > CHARYBDIS_AUTO_POINTER_LAYER_TRIGGER_THRESHOLD || abs(mouse_report.y) > CHARYBDIS_AUTO_POINTER_LAYER_TRIGGER_THRESHOLD) {
        if (auto_pointer_layer_timer == 0) {
            layer_on(LAYER_POINTER);
            #ifdef RGB_MATRIX_ENABLE
            rgb_matrix_mode_noeeprom(RGB_MATRIX_NONE);
            rgb_matrix_sethsv_noeeprom(HSV_GREEN);
            #endif // RGB_MATRIX_ENABLE
        }
        auto_pointer_layer_timer = timer_read();
    }
    return mouse_report;
}

void matrix_scan_user(void) {
    // 主要用来恢复自动切换鼠标层的哪些配置的，并把rgb颜色恢复刚开始的颜色
    if (auto_pointer_layer_timer != 0 && TIMER_DIFF_16(timer_read(), auto_pointer_layer_timer) >= CHARYBDIS_AUTO_POINTER_LAYER_TRIGGER_TIMEOUT_MS) {
        auto_pointer_layer_timer = 0;
        layer_off(LAYER_POINTER);
        #ifdef RGB_MATRIX_ENABLE
        rgb_matrix_mode_noeeprom(RGB_MATRIX_STARTUP_MODE);
        #endif // RGB_MATRIX_ENABLE
    };
    //不同层显示颜色指示灯
  uint8_t layer = get_highest_layer(layer_state);
  switch (layer) {
    case LAYER_LOWER:
      for(uint8_t i = 0; i <= 61; ++i) {		//遍历每一个灯
				rgb_matrix_set_color(i, 0, 255, 0);			//颜色变为绿色（R:0, G:255, B:0）
		}
      break;
    case LAYER_RAISE:
     for(uint8_t i = 0; i <= 61; ++i) {		//遍历每一个灯
				rgb_matrix_set_color(i, 255, 0, 0);			//颜色变为绿色（R:0, G:255, B:0）
		}
      break;
    case LAYER_POINTER:
      for(uint8_t i = 0; i <= 61; ++i) {		//遍历每一个灯
				rgb_matrix_set_color(i, 0, 0, 255);			//颜色变为绿色（R:0, G:255, B:0）
		}
      break;
    default:
      break;
  }
}
#endif // CHARYBDIS_AUTO_POINTER_LAYER_TRIGGER_ENABLE

//在图层变化时自动启用狙击模式，切换到LAYER_POINTER指针层启用阻击模式
//和自动启用阻击模式相关
#ifdef CHARYBDIS_AUTO_SNIPING_ON_LAYER
layer_state_t layer_state_set_user(layer_state_t state) {
    charybdis_set_pointer_sniping_enabled(layer_state_cmp(state, CHARYBDIS_AUTO_SNIPING_ON_LAYER));
    return state;
}
#    endif // CHARYBDIS_AUTO_SNIPING_ON_LAYER
#endif     // POINTING_DEVICE_ENABLE

//和rgb有关可以不用管
#ifdef RGB_MATRIX_ENABLE
// Forward-declare this helper function since it is defined in rgb_matrix.c.
void rgb_matrix_update_pwm_buffers(void);
#endif

